<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jckj.materialmanagement.mapper.MaterialInfoMapper">

    <select id="queryMaterialInfoListByCompanyId" resultType="com.jckj.materialmanagement.model.MaterialInfo">
        SELECT * FROM `m_material_info` where COMPANY_ID = #{companyId}
    </select>
</mapper>
